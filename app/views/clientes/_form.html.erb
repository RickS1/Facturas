<%= simple_form_for(@cliente) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :nombre %>
    <%= f.input :rfc %>
    <%= f.input :calle %>
    <%= f.input :numexterno %>
    <%= f.input :numinterno %>
    <%= f.input :colonia %>
    <%= f.input :codpostal %>
    <%= f.input :delegacion %>
    <%= f.input :ciudad %>
    <%= f.input :estado %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
